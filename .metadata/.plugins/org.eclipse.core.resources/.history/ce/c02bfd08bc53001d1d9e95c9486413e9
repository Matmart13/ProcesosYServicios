			// TODO: handle exception
		}
		BufferedReader in;
		try {
			InputStream inputStream = url.openStream();
			in = new BufferedReader(new InputStreamReader(inputStream));
			String inputLine;
			
		} catch (Exception e) {
			// TODO: handle exception
		}
	}

}
